{
  "expr":{
    "any":[
      "expr_list",
      "single_expr"
    ]
  },
  "identifier":{
    "node":"true",
    "regex":"([a-zA-Z0-9_\\-])+"
  },
  "non_literal":{
    "any":[
      "identifier",
      "invoke",
      "function"
    ]
  },
  "invokation":{
    "node":"true",
    "sequence":[
      "rvalue",
      "paren_open",
      "rvalues",
      "paren_close"
    ]
  },
  "lvalue":{
    "any":[
      "identifier"
    ]
  },
  "rvalue":{
    "any":[
      "literal",
      "lvalue",
      "assignment",
      "invokation"
    ]
  },
  "bind":{
    "node":"true",
    "sequence":[
      "rvalue",
      "dot",
      "identifier"
    ]
  },
  "dot":{
    "match":"."
  },
  "dereference_operator":{
    "match":"*"
  },
  "assignment_operator":{
    "match":"="
  },
  "assignment":{
    "node":"true",
    "sequence":[
      "identifier",
      "assignment_operator",
      "literal"
    ]
  },
  "indexer":{
    "sequence":[
      "bracket_open",
      "single_expr"
      "brace_close"
    ]
  },
  "bracket_open":{
    "match":"["
  },
  "bracket_close":{
    "match":"]"
  },
  "brace_open":{
    "match":"{"
  },
  "brace_close":{
    "match":"}"
  },
  "array":{
    "node":"true",
    "sequence":[
      "brace_open",
      "rvalues",
      "brace_close"
    ]
  },
  "rvalues":{
    "any":[
      "rvalue_list",
      "rvalue"
    ]
  },
  "rvalue_list":{
    "node":"true",
    "sequence":[
      "rvalue",
      "comma",
      "rvalues"
    ]
  },
  "literals":{
    "any":[
      "literal_list",
      "literal"
    ]
  },
  "literal_list":{
    "node":"true",
    "sequence":[
      "literal",
      "comma",
      "literals"
    ]
  },
  "single_expr":{
    "any":[
      "parentheses",
      "literal",
      "function_call"
    ]
   },
  "parentheses":{
    "node":"true",
    "sequence":[
      "paren_open",
      "expr",
      "paren_close"
    ]
  },
  "paren_open":{
    "match":"("

  },
  "paren_close":{
    "match":")"
  },
  "expr_list":{
    "node":"true",
    "sequence":[
      "single_expr",
      "comma",
      "expr"
    ]  
  },
  "comma":{
    "match":","
  },
  "literal":{
    "any":[
      "string",
      "integer"      
    ]
  },
  "string":{
    "any":[
      "single_quote_string",
      "double_quote_string"
    ]
  },
  "integer":{
    "node":"true",
    "regex":"(0|[1-9][0-9]*)"
  },
  "single_quote_string":{
    "node":"true",
    "regex":"'(([^']|\\')*)'",
    "replace":"1"   
  },
  "double_quote_string":{
    "node":"true",
    "regex":"\"(([^\"]|\\\")*)\"",
    "replace":"1"   
  }
}