{
  "name":"json-lang",
  "parsers":[
    "ast_parse_match",
    "ast_parse_regex",
    "ast_parse_any",
    "ast_parse_sequence"
  ],
  "evaluators":{
    "string":"ast_json_eval_string",
    "number":"ast_json_eval_number",
    "object":"ast_json_eval_object",
    "array":"ast_json_eval_array",
    "boolean":"ast_json_eval_boolean",
    "null":"ast_json_eval_null",
    "key_value":"ast_json_eval_key_value",
    "key_value_list":"ast_json_eval_key_value_list",
    "value_list":"ast_json_eval_value_list"
  },
  "root_definition":"json",
  "definitions":{
    "json":{
      "any":[
        "object",
        "array"
      ]
    },
    "object":{
      "node":"true",
      "sequence":[
        "brace_open",
        "key_values",
        "brace_close"
      ]
    },
    "array":{
      "node":"true",
      "sequence":[
        "bracket_open",
        "values",
        "bracket_close"
      ]
    },
    "brace_open":{
      "match":"{"
    },
    "brace_close":{
      "match":"}"
    },
    "bracket_close":{
      "match":"]"
    },
    "bracket_open":{
      "match":"["
    },
    "key_values":{
      "any":[
        "key_value_list",
        "key_value"
      ]
    },
    "key_value":{
      "node":"true",
      "sequence":[
        "string",
        "colon",
        "value"
      ]
    },
    "key_value_list":{
      "node":"true",
      "sequence":[
        "key_value",
        "comma",
        "key_values"
      ]
    },
    "values":{
      "any":[
        "value_list",
        "value"
      ]
    },
    "value_list":{
      "node":"true",
      "sequence":[
        "value",
        "comma",
        "values"
      ]
    },
    "colon":{
      "match":":"
    },
    "comma":{
      "match":","
    },
    "value":{
      "any":[
        "string",
        "number",
        "object",
        "array",
        "boolean",
        "null"
      ]
    },
    "string":{
      "node":"true",
      "regex":"\"(([^\"]|\\\")*)\"",
      "replace":"1"   
    },
    "number":{
      "node":"true",
      "regex":"([1-9][0-9]*)([.][0-9]+([eE][+-]?[0-9]+)?)?",
      "replace":"1"
    },
    "boolean":{
      "node":"true",
      "regex":"true|false"
    },
    "null":{
      "node":"true",
      "match":"null"
    }
  }
}