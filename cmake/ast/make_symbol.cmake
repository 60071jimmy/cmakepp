
function(make_symbol)
  ref_get(symbol_count i)
  if(NOT i)
    function(make_symbol)
      ref_get(symbol_count i)
      math(EXPR i "${i} + 1")
      ref_set(symbol_count "${i}")
      return("symbol_${i}")
    endfunction()
    ref_set(symbol_count 1)
    return(symbol_1)
  endif()
  message(FATAL_ERROR "make_symbol")
 endfunction()